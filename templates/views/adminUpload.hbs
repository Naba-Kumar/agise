{{>adminhead}}
<div class="wrapper">
    {{>sidebar}}
    <div class="main">
        <nav class="navbar navbar-expand px-3 border-bottom">
            <button class="btn" id="sidebar-toggle" type="button">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse navbar">
                <div class="">
                    <h4 class="mt-2">Admin Dashboard</h4>
                </div>
            </div>
        </nav>
        <main class="content px-3 py-2">
            <div class="container-fluid">


                <!-- Table Element -->
                <form action="#" method="post">
                    

                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">Shapefile Name *[Exactly same as uploaded file]</label>
                                <input type="text" class="form-control" placeholder="e.g. https://google.com"
                                    id="lname">
                            </div>
                        </div>


                    </div>

                    <div class="row mt-3">
                        <div class="col-md-6">

                            <div class="form-group first">
                                <label for="lname">upload</label>
                                <input type="file" class="form-control" placeholder="Your Password" id="password">

                            </div>
                        </div>

                    </div>

                    <div class="row mt-3">
                        <div class="col-md-6">

                            <div class="form-group last mb-3">
                                <label for="password">Description*</label>
                                <textarea type="text" class="form-control" placeholder="Your Password"
                                    id="password"></textarea>
                            </div>
                        </div>

                    </div>

                    <input type="submit" value="Upload" class="btn px-5 btn-primary mt-2">
                    

                </form>

            </div>
        </main>


    </div>
</div>
{{>footer}}