{{>adminhead}}
<div class="wrapper">
    {{>sidebar}}
    <div class="main">
        <nav class="navbar navbar-expand px-3 border-bottom">
            <button class="btn" id="sidebar-toggle" type="button">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse navbar">
                <div class="">
                    <h4 class="mt-2">Admin Dashboard</h4>
                </div>
            </div>
        </nav>
        <main class="content px-3 py-2">
            <div class="container-fluid">


                <!-- Table Element -->
                <form action="#" method="post">
                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="fname">First Name *</label>
                                <input type="text" class="form-control" placeholder="e.g. John" id="fname">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">Last Name *</label>
                                <input type="text" class="form-control" placeholder="e.g. Smith" id="lname">
                            </div>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="fname">Mobile No *</label>
                                <input type="text" class="form-control" placeholder="e.g. John" id="fname">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">Organization Name *</label>
                                <input type="text" class="form-control" placeholder="e.g. Smith" id="lname">
                            </div>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="fname">Department *</label>
                                <input type="text" class="form-control" placeholder="e.g. John" id="fname">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">Designation *</label>
                                <input type="text" class="form-control" placeholder="e.g. Smith" id="lname">
                            </div>
                        </div>
                    </div>

                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">E-mail address *</label>
                                <input type="text" class="form-control" placeholder="e.g. https://google.com"
                                    id="lname">
                            </div>
                        </div>

                    </div>



                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">User Type *</label>
                                <input type="text" class="form-control" placeholder="e.g. https://google.com"
                                    id="lname">
                            </div>
                        </div>
                    </div>

                    <div class="row mt-3">
                        <div class="col-md-6">
                            <div class="form-group first">
                                <label for="lname">Data Requested *</label>
                                <input type="text" class="form-control " placeholder="e.g. https://google.com"
                                    id="lname">
                            </div>
                        </div>


                    </div>



                    <div class="row mt-3">
                        <div class="col-md-6">

                            <div class="form-group last mb-3">
                                <label for="password">Reason OF Request</label>
                                <textarea type="text" class="form-control  " placeholder="Your Password"
                                    id="password"></textarea>
                            </div>
                        </div>

                    </div>

                    <input type="submit" value="Accept" class="btn px-5 btn-primary mt-2">
                    <input type="submit" value="Reject" class="btn px-5 btn-primary mt-2">

            </div>

            </form>

    </div>
    </main>


</div>
</div>
{{>footer}}